{
    "name": "DBShortcuts",
    "version": "0.1",
    "description": "Duelingbook keyboard shortcuts",
    "manifest_version": 3,

    "web_accessible_resources": [ {
        "resources": ["dbshortcuts.js", "config.json"],
        "matches": ["https://www.duelingbook.com/*"]
    } ],
    "content_scripts": [ {
        "js": ["content_script.js"],
        "matches": ["https://www.duelingbook.com/*"]
    } ]
}
